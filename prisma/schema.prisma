generator client {
  provider = "prisma-client"
  output = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id String @id @default(uuid())
  key String
  lastStation String?
  createdAt DateTime @default(now())
  sessions Session[]
}

model Session {
  id String @id @default(uuid())
  userId String
  expiresAt DateTime
  user User @relation(fields: [userId], references: [id])
}